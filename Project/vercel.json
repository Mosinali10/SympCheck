{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/(?!api/)(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "functions": {
    "api/server.js": {
      "runtime": "vercel-node@latest"
    }
  }
}